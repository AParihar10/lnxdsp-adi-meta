 # CONFIG_IOSCHED_CFQ is not set
 CONFIG_ARCH_SC58X=y
 CONFIG_MACH_SC584_EZKIT=y
+CONFIG_ARCH_SRAM_ALLOC=y
 CONFIG_ARM_THUMBEE=y
 CONFIG_PREEMPT_VOLUNTARY=y
 CONFIG_HZ_250=y
@@ -110,8 +111,6 @@ CONFIG_SND_SC5XX_ADAU1979=y
 CONFIG_SND_SC5XX_ADAU1962=y
 CONFIG_STAGING=y
 CONFIG_R8712U=y
-CONFIG_ICC_DRIVER=y
-CONFIG_ICC=y
 # CONFIG_VIRTIO_MENU is not set
 # CONFIG_IOMMU_SUPPORT is not set
 CONFIG_EXT4_FS=y
diff --git a/arch/arm/configs/sc589-ezkit_defconfig b/arch/arm/configs/sc589-ezkit_defconfig
index e322093e63a3..00b573e8bf28 100644
--- a/arch/arm/configs/sc589-ezkit_defconfig
+++ b/arch/arm/configs/sc589-ezkit_defconfig
@@ -23,6 +23,7 @@ CONFIG_MODULE_UNLOAD=y
 # CONFIG_IOSCHED_CFQ is not set
 CONFIG_ARCH_SC58X=y
 CONFIG_MACH_SC589_EZKIT=y
+CONFIG_ARCH_SRAM_ALLOC=y
 CONFIG_ARM_THUMBEE=y
 CONFIG_PCI=y
 CONFIG_PCI_MSI=y
@@ -125,8 +126,6 @@ CONFIG_RTC_CLASS=y
 CONFIG_RTC_DRV_ADI2=y
 CONFIG_STAGING=y
 CONFIG_R8712U=y
-CONFIG_ICC_DRIVER=y
-CONFIG_ICC=y
 # CONFIG_IOMMU_SUPPORT is not set
 CONFIG_EXT4_FS=y
 # CONFIG_DNOTIFY is not set
diff --git a/arch/arm/configs/sc589-mini_defconfig b/arch/arm/configs/sc589-mini_defconfig
index 5c8e28cc1081..6937cc83ff2b 100644
